while(True):
    A, B, C = map(int, input().split())
    if A == 0:
        break
    if A > B and A > C:
        if A**2 == B**2 + C**2:
            print("right")
        else:
            print("wrong")
    elif B > A and B > C:
        if B**2 == A**2 + C**2:
            print("right")
        else:
            print("wrong")
    elif C > A and C > B:
        if C**2 == A**2 + B**2:
            print("right")
        else:
            print("wrong")
